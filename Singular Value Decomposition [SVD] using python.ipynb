   "source": [
    "# Singular-value decomposition\n",
    "from numpy import array\n",
    "from scipy.linalg import svd\n",
    "# define a matrix\n",
    "A = array([[1, 2], [3, 4], [5, 6]])\n",
    "print(\"A: \\n%s\" %A)\n",
    "# SVD\n",
    "U, s, VT = svd(A)\n",
    "print(\"\\nU: \\n%s\" %U)\n",
    "print(\"\\ns: \\n %s\" %s)\n",
    "print(\"\\nV^T: \\n %s\" %VT)"
   ]
