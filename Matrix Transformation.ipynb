    "import numpy as np\n",
    "\n",
    "x = np.array([[1,2], [3,4]])\n",
    "print(\"Original x: \\n%s \" %x)    # Prints \"[[1 2]\n",
    "                                 #         [3 4]]\"\n",
    "print(\"\\nTranspose of x: \\n%s\" %x.T)  # Prints \"[[1 3]\n",
    "                                      #          [2 4]]\"\n"
